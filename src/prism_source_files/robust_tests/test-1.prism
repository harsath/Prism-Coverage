INT global_var1 = 100;

FUNCTION INT add(INT val1, INT val2, BOOL val3) {
       IF (val3) {
             RETURN val1 + val2;
       } ELSE {
             // this means nothing, just for illustration
             RETURN val1 * 2 + val2 * 2;
       }
}

FUNCTION INT main() {
	INT result = add(5, 10, true);
	RETURN result;
}
